# LLM configuration
LLM_PROVIDER=openai
LLM_MODEL=gpt-4o-mini
LLM_API_KEY=your-openai-key
LLM_BASE_URL=https://api.openai.com/v1
OPENAI_API_KEY=your-openai-key
EMBEDDING_MODEL_NAME=text-embedding-3-small
EMBEDDING_BACKEND=openai  # set to "dummy" for offline/local testing

# Audio configuration
ASR_MODEL=whisper-small
TTS_VOICE=alloy
AUDIO_SAMPLE_RATE=16000

# LangSmith tracing
LANGSMITH_TRACING=false
LANGSMITH_API_KEY=

# Web search provider
WEB_SEARCH_URL=https://example-search-api.local
WEB_SEARCH_API_KEY=
